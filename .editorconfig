# top-most EditorConfig file
root = true

[*]
charset = utf-8
end_of_line = lf
insert_final_newline = true

[*.js]
indent_style = space
indent_size = 4

[*.html]
indent_style = tab

[*.jsp]
indent_style = tab

[src/{main,test}/java/**.java]
indent_style = tab
tab_width = 4
trim_trailing_whitespace = true

